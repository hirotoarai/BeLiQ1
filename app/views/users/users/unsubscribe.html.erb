<h4>退会ページ</h4>
<div class="container">
  <div class="row">
    <div class="text-center col-xs-12">
      <h4><%= link_to "マイページに戻る", mypage_path %></h4></br>
      <% if @user == current_user %>
		    <h4><%= link_to "退会する", withdraw_user_path(@user), method: :patch %></h4>
		  <% end %>
		  <% if admin_signed_in? %>
		    <h4><%= link_to "退会させる", withdraw_user_path(@user), method: :patch %></h4>
		  <% end %>
    </div>
  </div>
</div>